<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Helloworld Landingspage</title>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\HelloWorld\Block\Landingspage" name="landingsblock" template="Magenest_HelloWorld::landingspage.phtml" />
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magenest\HelloWorld\Block\Newproducts" name="productblock" template="Magenest_HelloWorld::newproducts.phtml" />
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magenest\HelloWorld\Block\Customer" name="customerblock" template="Magenest_HelloWorld::customer.phtml" />
        </referenceContainer>
    </body>
</page>